<script type="text/javascript">

if (wi_ismobile) {
  var fluctAdScript = fluctAdScript || {};
  fluctAdScript.cmd = fluctAdScript.cmd || [];
  (function() {
      var groupId = '1000147013';
      var unitId = '1000243012'
  
      var targetDomains = ["www.google.com", "www.google.co.jp", "www.yahoo.co.jp", "search.yahoo.co.jp"];
      if (!isTargetReferral()) {

        return;
      }

      function isTargetReferral() {
        for (var domain of targetDomains) {
          if (document.referrer.indexOf(domain) !== -1) {
            return true;
          }
        }
        return false;
      }

      function showInterstitial(divId) {
        var div = document.createElement('div');
        div.id = divId;
        document.body.appendChild(div);
        fluctAdScript.cmd.push(function (cmd) {
            function doBack() { history.back(); }
            cmd.requestAdByGroup(groupId)
                .display('#' + divId, unitId)
                .onRendered(function(res) {
                    res.container.parentNode.querySelector('img').addEventListener('click', doBack);
                });
        });
            
      }

      history.scrollRestoration = "manual";
      history.pushState({}, document.title, location.href);

      window.addEventListener('popstate', function(event) {
          var wrapperElemId = "fluct_async_wrapper";
          showInterstitial(wrapperElemId);
      });
    })();
}
</script>


