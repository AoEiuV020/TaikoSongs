<script type="text/javascript">
var fluct_int_inbound = false;
var fluct_int_cookies = document.cookie;
var fluct_int_cookiesArray = fluct_int_cookies.split(';');
for(var fluct_int_c of fluct_int_cookiesArray){
    var fluct_int_cArray = fluct_int_c.split('=');
    if (fluct_int_cArray[0] == '_wi_ins'){
        fluct_int_inbound = (wi_ismobile && fluct_int_cArray[1] && wi_group == 'N' && wi_isinbound == false) ? true : false;
    } else if (fluct_int_cArray[0] == '_wi_ins_s'){
        wi_random100 = 50;
    }
}
var fluct_int_ignite = (wi_ismobile && wi_random100 == 50 && wi_group == 'N' && wi_isinbound == false) ? true : false;
var fluct_dev_mode = location.hash;
if (fluct_dev_mode == '##fluct_int_inbound') {
  fluct_int_inbound = true; // always
} else if  (fluct_dev_mode == '##fluct_int_ignite') {
  fluct_int_ignite = true; // always
}

if (wi_device == 'ios'){
  fluct_int_ignite = false;
  fluct_int_inbound = false;
}

if (wi_device == 'and' && fluct_int_inbound) {
  var fluct_int_group_id = 1000088446;
  var fluct_int_unit_id = 1000132472;
} else if (wi_device == 'ios' && fluct_int_inbound) {
  var fluct_int_group_id = 1000088446;
  var fluct_int_unit_id = 1000132471;
} else if (wi_device == 'and') {
  var fluct_int_group_id = 1000155331;
  var fluct_int_unit_id = 1000252319;
} else if (wi_device == 'ios') {
  var fluct_int_group_id = 1000155330;
  var fluct_int_unit_id = 1000252318;
}

if (fluct_int_ignite || fluct_int_inbound) {
  document.write('<scr' + 'ipt type="text/javascript" src="https://cdn-fluct.sh.adingo.jp/f.js?G=' + fluct_int_group_id + '"></scr' + 'ipt>');
  window['adingo_fluct_yads_btn_bold_large'] = true;
}
</script>

<script type="text/javascript">
if (fluct_int_ignite || fluct_int_inbound) {
  if (typeof (adingoFluct) !== 'undefined') {
    adingoFluct.showAd(fluct_int_unit_id);
    (function() {
      const target = document.getElementsByClassName("adingoFluctInter")[0];
      const observer = new MutationObserver((mutations) => {
        mutations.forEach((mutation) => {
          if (document.getElementsByClassName("hidden-switch-class") != null) {
            const hid_sw_cl = document.getElementsByClassName("hidden-switch-class");
          if (mutation.target.style.display == "block") {
            for(i = 0; i < hid_sw_cl.length; i++){
              hid_sw_cl[i].style.visibility ="hidden";
            }         
          } else if (mutation.target.style.display == "none") {
            for(i = 0; i < hid_sw_cl.length; i++){
              hid_sw_cl[i].style.visibility ="visible";
            }
            observer.disconnect();
          }
          }
        });
      });
      const config = {
        attributes: true,
        childList: true,
        subtree: true,
        attributeFilter: ['style']
      };
      observer.observe(target, config);
    }());
  }
}
</script>

